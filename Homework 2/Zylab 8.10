user_input = input() #remove whitespace

no_spaces = user_input.split()

new_str = ''.join(no_spaces)

#print(new_str)

reversed_str = ""

for i in new_str:
  reversed_str = i + reversed_str

#print(reversed_str)

if new_str == reversed_str:
  print(user_input, "is a palindrome")
else:
  print(user_input, "is not a palindrome")

