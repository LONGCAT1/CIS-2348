def exact_change(input):
  pennies = 0
  nickels = 0
  dimes = 0
  quarters = 0
  dollars = 0
  holder = 0
  
  if input <= 0:
    return pennies, nickels, dimes, quarters, dollars = 0

  if input >= 100:
    holder = input % 100
    dollars = input / 100
    
    quarters = holder / 25
    holder %= 25

    dimes = holder / 10
    holder %= 10

    nickels = holder / 5
    holder %= 5

    pennies = holder

  else:

    dollars = 0
    
    quarters = holder / 25
    holder %= 25

    dimes = holder / 10
    holder %= 10

    nickels = holder / 5
    holder %= 5

    pennies = holder

  return pennies, nickels, dimes, quarters, dollars


